{
  "expo": {
    "name": "Goober",
    "slug": "Goober",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Icon_Goober.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/Icon_Goober.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Goober needs your location to find nearby rides and track your trip.",
        "NSLocationAlwaysUsageDescription": "Goober needs your location to track your trip in real-time."
      },
      "bundleIdentifier": "com.goober.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Icon_Goober.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "package": "com.goober.app"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Goober needs your location to find nearby rides and track your trip."
        }
      ]
    ],
    "web": {
      "favicon": "./assets/Icon_Goober.png"
    }
  }
}
